{
    "presets": [
        [
            "@babel/preset-env",
            {
                "corejs": 3.20,
                "useBuiltIns": "usage" 
            }
        ],
        [
            "@babel/preset-react",
            {
                "runtime": "automatic" 
            }
        ]
    ],
    "env": {
        "test": {
            "plugins": [
                "@babel/plugin-transform-modules-commonjs"
            ]
        }
    },
    "plugins": [
        "transform-object-rest-spread"
    ] 
}